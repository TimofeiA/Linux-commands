git config --global user.name "name"   ----add your github account name-----
git config --global user.email "email"
git config --global --list  ---check what config you have---
ssh-keygen -t ed25519 -C "email"
git remote set-url origin "ssh_link_from github"


git init
git status
git add .     git add file1.txt
git commit -m "Initial commit"
git log
git log -2  -------------show only 2 last commits(git log -1 show only last commit)----------------
git log --oneline
git shortlog
git log -p -1    extended log for last commit
git checkout file1.txt       ---return to a previous state of the file---
git clone
git branch two
git checkout two
